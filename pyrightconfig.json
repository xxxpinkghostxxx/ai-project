{
  "include": [
    "src"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/venv",
    "**/.venv"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "reportUnknownVariableType": "none",
  "reportUnknownMemberType": "none",
  "pythonVersion": "3.10",
  "typeCheckingMode": "basic",
  "venvPath": ".",
  "venv": "venv"
}

